pcm.rawjack {
    type jack
    playback_ports {
        0 alsa_loop:input_1
        1 alsa_loop:input_2
    }
#    capture_ports {
#        0 system:capture_1
#        1 system:capture_2
#    }
}

pcm.jack {
    type plug
    slave { pcm "rawjack" }
    hint {
    description "JACK Audio Connection Kit"
    }
}

pcm.!default {
    type plug
    slave { pcm "rawjack" }
}
